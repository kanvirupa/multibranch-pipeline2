pipeline {   
    agent any

    stages {   
        stage('main') { 
            steps { 
               sh 'echo "This is master branch"' 
            }
        }
     
        stage('dev') { 
            steps { 
               sh 'echo "dev application change..."'
            }
        }

        stage('hotfix1') { 
             steps { 
                sh 'echo "hotfix application...change..."'
            }
        }  
    }
}

